{
  "packageManager": "yarn@2.4.3",
  "name": "@ubahnchen/svgo",
  "version": "3.0.2",
  "type": "module",
  "description": "Nodejs-based tool for optimizing SVG vector graphics files",
  "license": "MIT",
  "keywords": [
    "svgo",
    "svg",
    "optimize",
    "minify"
  ],
  "homepage": "https://github.com/svg/svgo",
  "bugs": {
    "url": "https://github.com/svg/svgo/issues"
  },
  "author": {
    "name": "Kir Belevich",
    "email": "kir@belevi.ch",
    "url": "https://github.com/deepsweet"
  },
  "contributors": [
    {
      "name": "Sergey Belov",
      "email": "peimei@ya.ru",
      "url": "https://github.com/arikon"
    },
    {
      "name": "Lev Solntsev",
      "email": "lev.sun@ya.ru",
      "url": "https://github.com/GreLI"
    },
    {
      "name": "Bogdan Chadkin",
      "email": "trysound@yandex.ru",
      "url": "https://github.com/TrySound"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/svg/svgo.git"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/svgo"
  },
  "main": "./dist/svgo.js",
  "bin": "./bin/svgo",
  "types": "./dist/svgo.d.ts",
  "files": [
    "bin",
    "lib",
    "plugins",
    "dist",
    "!**/*.test.js"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "test": "node --loader ts-node/esm --es-module-specifier-resolution=node --experimental-vm-modules --no-warnings ../../node_modules/.bin/jest --config ../../jest.config.cjs --rootDir=./",
    "lint": "eslint --ignore-path .gitignore . && prettier --check \"**/*.js\" --ignore-path .gitignore",
    "fix": "eslint --ignore-path .gitignore --fix . && prettier --write \"**/*.js\" --ignore-path .gitignore",
    "typecheck": "tsc",
    "test-browser": "tsup && node --loader ts-node/esm --es-module-specifier-resolution=node --experimental-vm-modules --no-warnings ./test/browser.ts",
    "test-regression": "node ./test/regression-extract.js && NO_DIFF=1 node ./test/regression.js",
    "prepublishOnly": "rm -rf dist && rollup -c"
  },
  "dependencies": {
    "@trysound/sax": "0.2.0",
    "commander": "^7.2.0",
    "css-select": "^5.1.0",
    "css-tree": "^2.2.1",
    "csso": "^5.0.5",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@types/css-tree": "^2.0.0",
    "@types/csso": "^5.0.0",
    "@types/jest": "^29.1.1",
    "del": "^6.0.0",
    "jest": "^29.1.2",
    "node-fetch": "^2.6.2",
    "pixelmatch": "^5.2.1",
    "playwright": "^1.14.1",
    "pngjs": "^6.0.0",
    "prettier": "^2.7.1",
    "tar-stream": "^2.2.0",
    "typescript": "^4.8.4"
  }
}
